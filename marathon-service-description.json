{
  "id": "/micro/uuid-service",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 3001,
        "hostPort": 0,
        "labels": {
          "VIP_0": "/micro-uuid-service:3000"
        },
        "protocol": "tcp",
        "servicePort": 10102,
        "name": "uuid-service"
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "mesosplayground/nsc",
      "forcePullImage": false,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 0.1,
  "disk": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "MESOS_HTTP",
      "path": "/act?role=health&cmd=ping&ver=1"
    }
  ],
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 128,
  "gpus": 0,
  "networks": [
    {
      "name": "dcos",
      "mode": "container"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "fetch": [
    {
      "uri": "https://github.com/mesos-playground/UuidService/releases/download/2017.09.22-1/bundle.tgz"
    }
  ],
  "constraints": []
}
